<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="borderPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.control.MainController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Accesso">
            <items>
                  <MenuItem fx:id="mnuLogin" mnemonicParsing="false" onAction="#doMnuLogin" text="Login" />
              <MenuItem fx:id="mnuChiudi" mnemonicParsing="false" onAction="#doMnuChiudi" text="Chiudi" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Supermercato">
            <items>
              <MenuItem mnemonicParsing="false" text="Aggiungi" />
                  <MenuItem mnemonicParsing="false" text="Tutti" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Prodotto">
              <items>
                <MenuItem mnemonicParsing="false" text="Aggiungi" />
                  <MenuItem mnemonicParsing="false" text="Tutti" />
                  <MenuItem mnemonicParsing="false" text="Filtro SuperMercato" />
              </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Aiuto">
            <items>
                  <MenuItem mnemonicParsing="false" text="?" />
              <MenuItem fx:id="mnuInformazioni" mnemonicParsing="false" onAction="#doMnuInformazioni" text="informazioni" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
